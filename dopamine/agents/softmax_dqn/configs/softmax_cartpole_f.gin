# Hyperparameters for a simple C51-style Cartpole agent. The hyperparameters
# chosen achieve reasonable performance.
import dopamine.agents.dqn.dqn_agent
import dopamine.agents.softmax_dqn.softmax_dqn_agent
import dopamine.agents.rainbow.rainbow_agent
import dopamine.discrete_domains.gym_lib
import dopamine.discrete_domains.run_experiment
import dopamine.replay_memory.prioritized_replay_buffer
import gin.tf.external_configurables

SoftMaxDQNAgent.observation_shape = %gym_lib.CARTPOLE_OBSERVATION_SHAPE
SoftMaxDQNAgent.observation_dtype = %gym_lib.CARTPOLE_OBSERVATION_DTYPE
SoftMaxDQNAgent.stack_size = %gym_lib.CARTPOLE_STACK_SIZE
SoftMaxDQNAgent.network = @gym_lib.small_cartpole_rainbow_network
SoftMaxDQNAgent.num_atoms = 51
SoftMaxDQNAgent.vmax = 100.
SoftMaxDQNAgent.gamma = 0.99
SoftMaxDQNAgent.update_horizon = 1
SoftMaxDQNAgent.min_replay_history = 500
SoftMaxDQNAgent.update_period = 4
SoftMaxDQNAgent.target_update_period = 100
SoftMaxDQNAgent.epsilon_fn = @dqn_agent.identity_epsilon
SoftMaxDQNAgent.epsilon_train = 0.1
SoftMaxDQNAgent.replay_scheme = 'uniform'
SoftMaxDQNAgent.tf_device = '/gpu:0'  # use '/cpu:*' for non-GPU version
SoftMaxDQNAgent.optimizer = @tf.train.AdamOptimizer()

tf.train.AdamOptimizer.learning_rate = 0.00001
tf.train.AdamOptimizer.epsilon = 0.0003125

create_gym_environment.environment_name = 'CartPole'
create_gym_environment.version = 'v0'
run_feature_experiment.create_agent.agent_name = 'softmax_dqn'
FeatureRunner.create_environment_fn = @gym_lib.create_gym_environment
FeatureRunner.num_iterations = 500
FeatureRunner.training_steps = 1000
FeatureRunner.evaluation_steps = 1000
FeatureRunner.max_steps_per_episode = 200  # Default max episode length.

WrappedPrioritizedReplayBuffer.replay_capacity = 50000
WrappedPrioritizedReplayBuffer.batch_size = 128
